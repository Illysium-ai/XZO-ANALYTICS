 # PostgreSQL Configuration
DESTINATION__POSTGRES__CREDENTIALS__HOST=your_postgres_host
DESTINATION__POSTGRES__CREDENTIALS__PORT=your_postgres_port
DESTINATION__POSTGRES__CREDENTIALS__DATABASE=your_postgres_database
DESTINATION__POSTGRES__CREDENTIALS__USERNAME=your_postgres_username
DESTINATION__POSTGRES__CREDENTIALS__PASSWORD=your_postgres_password

# S3 Configuration
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
S3_BUCKET_NAME=your_bucket_name
S3_PREFIX=optional_s3_prefix

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# dbt/Dagster runtime profile generation
DBT_PROFILE_NAME=apollo-snowflake
DBT_TARGET=dev
DBT_THREADS=8

# Snowflake (use key pair auth)
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_ROLE=your_role
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_WAREHOUSE=your_warehouse
# Provide one of the following:
# SNOWFLAKE_PRIVATE_KEY_PEM="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
# or base64-encoded PEM
# SNOWFLAKE_PRIVATE_KEY_B64=base64_encoded_pem

# Optional override (policy is to omit schema by default)
# DBT_SCHEMA=override_schema